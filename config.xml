<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="3" id="com.pangeranweb.joglosemar" version="0.2.3" xmlns="http://www.w3.org/ns/widgets" xmlns:intelxdk="http://xdk.intel.com/ns/v1">
    <intelxdk:version value="1.1" />
    <intelxdk:cordova-cli version="6.2.0" />
    <name>Joglosemar</name>
    <description>Tiket antar kota jawa</description>
    <author>Pangeran web</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" height="36" platform="android" src="package-assets/icon-36.png" width="36" />
    <icon density="mdpi" height="48" platform="android" src="package-assets/icon-48.png" width="48" />
    <icon density="hdpi" height="72" platform="android" src="package-assets/icon-72.png" width="72" />
    <icon density="xhdpi" height="96" platform="android" src="package-assets/icon-96.png" width="96" />
    <intelxdk:plugin intelxdk:checksum="c0146fbc" intelxdk:name="StatusBar" intelxdk:type="file" intelxdk:value="cordova-plugin-statusbar" intelxdk:version="2.1.3" />
    <intelxdk:plugin intelxdk:checksum="2373ccea" intelxdk:name="Device" intelxdk:type="file" intelxdk:value="cordova-plugin-device" intelxdk:version="1.1.2" />
    <intelxdk:plugin intelxdk:checksum="47137083" intelxdk:name="Splashscreen" intelxdk:type="file" intelxdk:value="cordova-plugin-splashscreen" intelxdk:version="3.2.2" />
    <intelxdk:plugin intelxdk:name="Cordova Network Whitelist Plugin (added by Intel XDK)" intelxdk:value="cordova-plugin-whitelist" intelxdk:version="1.2.2" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="android-targetSdkVersion" value="21" />
    <preference name="android-installLocation" value="auto" />
    <preference name="Fullscreen" value="false" />
    <platform name="android">
        <preference name="Orientation" value="portrait" />
    </platform>
    <preference name="android-signed" value="true" />
    <intelxdk:crosswalk version="19" />
    <preference name="debuggable" value="false" />
    <platform name="ios">
        <preference name="AutoHideSplashScreen" value="true" />
        <preference name="FadeSplashScreen" value="false" />
        <preference name="FadeSplashScreenDuration" value="2" />
        <preference name="ShowSplashScreenSpinner" value="false" />
        <preference name="StatusBarOverlaysWebView" value="false" />
        <preference name="StatusBarBackgroundColor" value="#000000" />
        <preference name="StatusBarStyle" value="lightcontent" />
    </platform>
    <platform name="android">
        <preference name="SplashMaintainAspectRatio" value="false" />
    </platform>
    <intelxdk:crosswalk xmlns:intelxdk="http://xdk.intel.com/ns/v1" xwalk-command-line="--disable-pull-to-refresh-effect" />
    <intelxdk:crosswalk xmlns:intelxdk="http://xdk.intel.com/ns/v1" xwalk-command-line="--ignore-gpu-blacklist" />
    <engine name="android" spec="^6.2.3" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="^2.3.0">
        <variable name="XWALK_VERSION" value="23+" />
        <variable name="XWALK_LITEVERSION" value="xwalk_core_library_canary:17+" />
        <variable name="XWALK_COMMANDLINE" value="--disable-pull-to-refresh-effect" />
        <variable name="XWALK_MODE" value="embedded" />
        <variable name="XWALK_MULTIPLEAPK" value="true" />
    </plugin>
    <plugin name="cordova-plugin-device" spec="1.1.2" />
    <plugin name="cordova-plugin-splashscreen" spec="3.2.2" />
    <plugin name="cordova-plugin-statusbar" spec="2.1.3" />
    <plugin name="cordova-plugin-whitelist" spec="1.2.2" />
